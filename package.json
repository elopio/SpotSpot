{
  "name": "spotspot",
  "productName": "SpotSpot",
  "version": "2.0.1",
  "description": "Spotify mini-player for macOS",
  "homepage": "http://wstone.io/SpotSpot/",
  "main": "src/main.js",
  "scripts": {
    "prettier": "prettier --config .prettierrc --write src/**/*.js",
    "start": "SPOTSPOT_ENV=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint src"
  },
  "repository": "https://github.com/will-stone/SpotSpot",
  "bugs": {
    "url": "https://github.com/will-stone/SpotSpot/issues"
  },
  "keywords": ["Electron", "Spotify", "mini-player", "macOS"],
  "author": "Will Stone",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.6",
    "electron-prebuilt-compile": "1.6.11",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "prettier": "^1.9.1"
  },
  "dependencies": {
    "about-window": "^1.6.3",
    "electron-compile": "^6.4.1",
    "font-awesome": "^4.7.0",
    "minireset.css": "^0.0.3",
    "spotify-node-applescript": "^1.1.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": ["squirrel"],
        "darwin": ["zip"],
        "linux": ["deb", "rpm"]
      },
      "electronPackagerConfig": {
        "icon": "src/images/icon/icon.png.icns"
      },
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
